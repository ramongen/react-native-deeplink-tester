{
  "expo": {
    "name": "react-native-deeplink-tester",
    "slug": "react-native-deeplink-tester",
    "scheme": 'tester',
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.ramonroffon.reactnativedeeplinktester",
		"intentFilters": [
			{
				"action": "VIEW",
				"category": ["BROWSABLE", "DEFAULT"],
				"data": [{ "scheme": "https", "host": "tester.com", "pathPrefix": "/" }]
			}
		]
	},
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
